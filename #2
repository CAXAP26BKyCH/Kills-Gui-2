-- Scripts:

local function VINPDS_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local function updateKills()
		local player = game.Players.LocalPlayer
		local kills = player.leaderstats.Kills.Value
		label.Text = "Kills: " .. kills
	end
	
	local player = game.Players.LocalPlayer
	player.leaderstats.Kills.Changed:Connect(updateKills)
	
	updateKills()
end
coroutine.wrap(VINPDS_fake_script)()
local function ZNFA_fake_script() -- Frame.ResetOnSpawn 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Parent.ResetOnSpawn = false
end
coroutine.wrap(ZNFA_fake_script)()
local function XXUAO_fake_script() -- Frame.Draggable 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(XXUAO_fake_script)()
